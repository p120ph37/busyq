{
  "name": "busyq",
  "version-string": "0.1.0",
  "description": "Single-binary bash+curl+jq for distroless containers",
  "dependencies": [
    "busyq-bash",
    "busyq-coreutils",
    "busyq-jq",
    "ncurses",
    "oniguruma",
    {
      "name": "busyq-curl",
      "default-features": false
    }
  ],
  "features": {
    "ssl": {
      "description": "SSL/TLS support via mbedtls with embedded CA certificates",
      "dependencies": [
        "mbedtls",
        {
          "name": "busyq-curl",
          "features": ["ssl"]
        }
      ]
    }
  }
}
