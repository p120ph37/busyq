{
  "name": "busyq",
  "version-string": "0.2.0",
  "description": "Single-binary bash+curl+jq+coreutils+tools for distroless containers",
  "dependencies": [
    "busyq-bash",
    "busyq-coreutils",
    "busyq-jq",
    "ncurses",
    "oniguruma",
    {
      "name": "busyq-curl",
      "default-features": false
    },
    "busyq-gawk",
    "busyq-sed",
    "busyq-grep",
    "busyq-diffutils",
    "busyq-findutils",
    "busyq-ed",
    "busyq-patch",
    "busyq-tar",
    "busyq-gzip",
    "busyq-bzip2",
    "busyq-xz",
    "busyq-cpio",
    "busyq-lzop",
    "busyq-zip",
    "busyq-bc",
    "busyq-less",
    "busyq-strings",
    "busyq-time",
    "busyq-dos2unix",
    "busyq-sharutils",
    "busyq-reset",
    "busyq-which",
    "busyq-wget",
    "busyq-netcat",
    "busyq-iputils",
    "busyq-hostname",
    "busyq-whois",
    "busyq-procps",
    "busyq-psmisc",
    "busyq-lsof"
  ],
  "features": {
    "ssl": {
      "description": "SSL/TLS support via mbedtls with embedded CA certificates",
      "dependencies": [
        "mbedtls",
        {
          "name": "busyq-curl",
          "features": ["ssl"]
        }
      ]
    }
  }
}
